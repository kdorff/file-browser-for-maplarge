{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES2015",
        "outDir": "../wwwroot/js",
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ]
}